{
	"name": "@deanrih/ts-lib-env-typebox",
	"description": "A helper library for parsing env with a schema with enforced validation.",
	"version": "1.0.1",
	"type": "module",
	"license": "MIT",
	"homepage": "https://deanrih.id/projects/self/ts-lib-env-typebox",
	"readme": "./README.md",
	"author": {
		"name": "deanrih",
		"url": "https://github.com/deanrih"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/deanrih/ts-lib-env-typebox"
	},
	"bugs": {
		"url": "https://github.com/deanrih/ts-lib-env-typebox/issues"
	},
	"keywords": [
		"ENV",
		"ENV Validation",
		"ENV Schema",
		"TypeBox"
	],
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "bun run --watch ./example/index.example.ts",
		"case:dev": "bun run --watch ./example/case.example.ts",
		"test": "bun test",
		"build": "bunx --bun bunup",
		"release": "bun run test && bun run build && bun publish"
	},
	"devDependencies": {
		"@biomejs/biome": "latest",
		"@types/bun": "latest",
		"bunup": "latest",
		"mitata": "latest",
		"typescript": "latest"
	},
	"peerDependencies": {
		"@sinclair/typebox": "latest"
	}
}